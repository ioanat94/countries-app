{
  "name": "fast-track-typescript-redux-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.1.3",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "postcss": "^8.4.16",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-loading-skeleton": "^3.1.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-router-refreshable": "^0.4.0",
    "react-scripts": "^3.4.0",
    "redux": "^4.2.0",
    "tailwindcss": "^3.1.8",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "ts-coverage": "typescript-coverage-report",
    "watch:tailwind": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./src/output.css",
    "start": "react-scripts start",
    "dev": "run-p watch:tailwind start",
    "prebuild": "react-scripts build",
    "build": "run-p tailwind prebuild",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --fix '**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "typescript-coverage-report": "^0.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint --fix"
    ]
  }
}
